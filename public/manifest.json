{
  "name": "Resource download",
  "description": "an chrome extension for grabbing resources",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "/test_128.png",
    "32": "/test_128.png",
    "48": "/test_128.png",
    "128": "/test_128.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "options_page": "options/index.html",
  "permissions": ["storage", "activeTab", "scripting", "tabs", "http://*/*"],
  "host_permissions": ["*://*/*"]
}
